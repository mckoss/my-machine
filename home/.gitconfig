[user]
  name = Mike Koss
  email = mckoss@startpad.org

[github]
  user = mckoss

[core]
    excludesfile = /Users/mike/.gitignore
    autocrlf = input

[alias]
  hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
  type = cat-file -t
  dump = cat-file -p
  co = checkout
  ci = checkin
  st = status
